{
"name": "superGestures",
"manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
			"matches": ["<all_urls>"],
      "js": ["jquery.js", "mouseTrack.js"],
			"all_frames": true,
			"run_at": "document_start"
    }
  ],
"version": "0.1",
"description": "a very simple mouse gestures program"
}